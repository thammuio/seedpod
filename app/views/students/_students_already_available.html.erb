<div class="col-lg-9 col-md-offset-1">
  <div class="panel panel-default">
    <div class="panel-title" style="text-align: center;">
      Student are already in this course
    </div>
    <div class="panel-body">
      <div class="table-responsive">
        <table class="table table-striped table-bordered table-hover all_student">
          <thead>
            <tr>
              <th>Email</th>
              <th>Student Name</th>
            </tr>
          </thead>
          <tbody>
            <% @student_course.student.each do |course_student| %>
              <tr>
                <td>
                  <%= course_student.email %>
                </td>
                <td>
                  <%= course_student.student_name %>
                </td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>